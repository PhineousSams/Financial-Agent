<div class="mb-6">
  <h1 class="text-2xl font-semibold flex items-center">
    <i class="fa-regular fa-history mr-2"></i>
    Session Logs
    <small class="ml-2 text-gray-600 font-normal text-base">
      List of session activities
    </small>
  </h1>
</div>
<!-- Main Content -->
<div class="bg-white shadow-md rounded-lg overflow-hidden">
  <div class="p-6">
    <div class="my-overflow-div">
      <.live_component
        module={FinincialToolWeb.Helps.ISearchComponent}
        id="ISearchComponent"
        params={@params}
      />
      <input
        type="hidden"
        id="csrf"
        name="_csrf_token"
        value={Plug.CSRFProtection.get_csrf_token()}
      />
      <table class="table min-w-full divide-y divide-gray-200">
        <thead class="bg-gray-800 text-white">
          <tr class="table-row">
            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              Date
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              User Name
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              Session
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              IP Address
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              Description
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              Browser Name
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              Browser Details
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              Device Name
            </th>

            <th
              scope="col"
              class="px-6 py-3 text-left text-xs font-medium uppercase tracking-wider"
            >
              System Name
            </th>
          </tr>
        </thead>

        <tbody class="bg-white divide-y divide-gray-200">
          <%= for phin <- @data do %>
            <tr class="hover:bg-gray-50">
              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.inserted_at %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.username %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.session_id %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.ip_address %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.description %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.full_browser_name %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.browser_details %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.device_type %>
              </td>

              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
                <%= phin.system_platform_name %>
              </td>
            </tr>
          <% end %>
        </tbody>

        <%= if @data_loader do %>
          <tr>
            <td colspan="9" class="px-6 py-4 text-center">
              <div class="inline-block animate-spin rounded-full h-8 w-8 border-b-2 border-gray-900">
              </div>
            </td>
          </tr>
        <% else %>
          <%= if Enum.empty?(@data) do %>
            <tr>
              <td colspan="9" class="px-6 py-4 text-center text-gray-500">
                No data available in table
              </td>
            </tr>
          <% end %>
        <% end %>
      </table>

      <.live_component
        module={FinincialToolWeb.Helps.PaginationComponent}
        id="PaginationComponent"
        params={@params}
        pagination_data={@data}
      />
    </div>
  </div>
</div>
